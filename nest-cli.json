{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/camvolleyball-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/camvolleyball-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/camvolleyball-api",
  "projects": {
    "camvolleyball-api": {
      "type": "application",
      "root": "apps/camvolleyball-api",
      "entryFile": "main",
      "sourceRoot": "apps/camvolleyball-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/camvolleyball-api/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "post-service": {
      "type": "application",
      "root": "apps/post-service",
      "entryFile": "main",
      "sourceRoot": "apps/post-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post-service/tsconfig.app.json"
      }
    }
  }
}